<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/activity_aproval_assistant"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.wyu.iwork.view.activity.AprovalAssistantActivity">

    <include layout="@layout/layout_erp_edit_title"/>
    <android.support.design.widget.TabLayout
        android:id="@+id/activity_aproval_assistant_tablayout"
        android:layout_width="match_parent"
        android:layout_height="@dimen/px100"
        app:tabSelectedTextColor="@color/blue"
        app:tabTextColor="@color/black_9FABB2"
        app:tabIndicatorColor="@color/blue"
        android:background="@color/white"
        app:tabTextAppearance="@style/MyTabLayoutTextAppearance"/>

    <android.support.v4.view.ViewPager
        android:id="@+id/activity_aproval_assistant_viewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

</LinearLayout>
