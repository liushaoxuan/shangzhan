<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_potential_open_sea_detail"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.wyu.iwork.view.activity.PotentialOpenSeaDetailActivity">

    <include layout="@layout/custom_back_toolbar"/>

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:scrollbars="none">

        <com.zhy.autolayout.AutoLinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <com.wyu.iwork.widget.CustomCrmItem
                android:id="@+id/open_sea_detail_name"
                android:layout_marginTop="23px"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <com.wyu.iwork.widget.CustomCrmItem
                android:id="@+id/open_sea_detail_phone"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <com.wyu.iwork.widget.CustomCrmItem
                android:id="@+id/open_sea_detail_time"
                android:layout_marginTop="23px"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <com.wyu.iwork.widget.CustomCrmItem
                android:id="@+id/open_sea_detail_case"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <com.wyu.iwork.widget.CustomCrmItem
                android:id="@+id/open_sea_detail_person"
                android:layout_marginTop="23px"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

            <com.wyu.iwork.widget.CustomCrmItem
                android:id="@+id/open_sea_detail_way"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"/>

        </com.zhy.autolayout.AutoLinearLayout>

    </ScrollView>

</com.zhy.autolayout.AutoLinearLayout>
