    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_meeting_list"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/f4f9fc"
    android:orientation="vertical"
    tools:context="com.wyu.iwork.view.activity.MeetingListActivity">

    <include layout="@layout/custom_back_toolbar"/>

    <include layout="@layout/layout_schedule_header"/>

    <android.support.v4.view.ViewPager
        android:id="@+id/meeting_list_viewpager"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>

</com.zhy.autolayout.AutoLinearLayout>
