                android:id="@+id/activity_build_cargo_location_storeName"
                android:layout_width="match_parent"
                android:layout_height="@dimen/px97"
                app:ShowStar="false"
                app:showArrow="false"
                app:showInput="false"
                android:layout_marginTop="@dimen/px1"
                app:StarSize="@dimen/px36"
