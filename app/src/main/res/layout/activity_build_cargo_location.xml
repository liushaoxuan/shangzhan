                app:RightText=""/>

            <com.wyu.iwork.widget.CustomeViewGroup
                android:id="@+id/activity_build_cargo_location_releaser"
                android:layout_width="match_parent"
                android:layout_height="@dimen/px97"
                app:ShowStar="false"
                app:showArrow="false"
                android:layout_marginTop="@dimen/px24"
                app:StarSize="@dimen/px36"
                app:StarColor="@color/red"
                app:NameSize="@dimen/px32"
                app:NameColor="@color/black6"
                app:NameText="创建人"
                app:InputColor="@color/black9"
                app:InputSize="@dimen/px30"
                app:RightTextSize="@dimen/px30"
                app:RightTextColor="@color/black9"
                app:RightText="RNG0120023"/>

            <com.wyu.iwork.widget.CustomeViewGroup
                android:id="@+id/activity_build_cargo_location_releaseTime"
                android:layout_width="match_parent"
                android:layout_height="@dimen/px97"
                app:ShowStar="false"
                app:showArrow="false"
                android:layout_marginTop="@dimen/px1"
                app:StarSize="@dimen/px36"
                app:StarColor="@color/red"
                app:NameSize="@dimen/px32"
                app:NameColor="@color/black6"
                app:NameText="创建时间"
                app:InputColor="@color/black9"
                app:InputSize="@dimen/px30"
                app:RightTextSize="@dimen/px30"
                app:RightTextColor="@color/black9"
                app:RightText="RNG0120023"/>

