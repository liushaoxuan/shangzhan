            android:layout_height="wrap_content"
            android:orientation="vertical">
            <com.wyu.iwork.widget.CustomeViewGroup
                android:id="@+id/activity_build_cargo_location_cargoNum"
                android:visibility="gone"
                android:layout_width="match_parent"
                android:layout_height="@dimen/px97"
                app:ShowStar="true"
                app:showArrow="false"
                app:showInput="true"
                android:layout_marginTop="@dimen/px24"
                app:StarSize="@dimen/px36"
                app:StarColor="@color/red"
                app:NameSize="@dimen/px32"
                app:NameColor="@color/black6"
                app:NameText="货位编号"
                app:InputColor="@color/black9"
                app:InputSize="@dimen/px30"
                app:RightTextSize="@dimen/px30"
