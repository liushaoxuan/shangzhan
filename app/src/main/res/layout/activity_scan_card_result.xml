<?xml version="1.0" encoding="utf-8"?>
<com.zhy.autolayout.AutoLinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_scan_card_result"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    tools:context="com.wyu.iwork.view.activity.ScanCardResultActivity"
    android:background="@color/colorF4F9FC">

    <include layout="@layout/custom_back_toolbar"/>

    <View
        android:layout_width="match_parent"
        android:layout_height="30px"/>

    <include layout="@layout/layout_card_myself"/>

    <TextView
        android:id="@+id/tv_add_card"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/bg_25b0ff_30px"
        android:layout_marginTop="100px"
        android:layout_marginRight="24px"
        android:layout_marginLeft="24px"
        android:text="添加至商栈 · 云办公微名片"
        android:gravity="center"
        android:padding="27px"
        android:textSize="36px"
        android:textColor="@color/white"
        />
</com.zhy.autolayout.AutoLinearLayout>
